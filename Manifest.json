{
	"manifest_version": 2,

	"name": "Modify-CSS-HTML",
	"version": "0.1.0",
	"description": "Improve Browsing",

	"content_scripts": [
		{
			"css": ["subtitles.css"],
			"js": ["subtitles.js"],
			"matches": ["https://www.youtube.com/*"]
		},
		{
			"js": ["danmaku.js"],
			"matches": ["https://www.bilibili.com/bangumi/play/*"]
		}
	]

}